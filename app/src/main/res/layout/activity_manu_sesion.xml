<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E9F6B9B9"
    tools:context=".app.ui.views.ManuSesion">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewSesion"
        android:layout_width="603dp"
        android:layout_height="268dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.497">

    </androidx.recyclerview.widget.RecyclerView>

    <ProgressBar
        android:id="@+id/progressBarseguridad"
        style="?android:attr/progressBarStyle"
        android:layout_width="357dp"
        android:layout_height="197dp"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerViewSesion"
        app:layout_constraintEnd_toEndOf="@+id/recyclerViewSesion"
        app:layout_constraintStart_toStartOf="@+id/recyclerViewSesion"
        app:layout_constraintTop_toTopOf="@+id/recyclerViewSesion" />

</androidx.constraintlayout.widget.ConstraintLayout>